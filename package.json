{
  "name": "hacker-news-eggjs-umi",
  "version": "1.0.0",
  "description": "hacker news with eggjs and umi ssr",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "cross-env": "^7.0.3",
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "egg-view-assets": "^1.8.0",
    "egg-view-nunjucks": "^2.3.0",
    "mime": "^3.0.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-hacker-news-eggjs-umi",
    "stop": "egg-scripts stop --title=egg-server-hacker-news-eggjs-umi",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "start-web": "cross-env APP_ROOT=app/web umi dev",
    "build-web": "cross-env APP_ROOT=app/web umi build",
    "postinstall-web": "cross-env APP_ROOT=app/web umi generate tmp",
    "prettier-web": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test-web": "cross-env APP_ROOT=app/web umi-test",
    "test:coverage-web": "cross-env APP_ROOT=app/web umi-test --coverage"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "ashoka",
  "license": "MIT"
}
